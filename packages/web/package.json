	"license": "MIT",
	"scripts": {
		"start": "gatsby develop",
		"develop": "gatsby develop && cp -a ./src/public ./public",
		"clean": "gatsby clean",
		"copy": "cp -a src/public/ public/",
		"build": "yarn run scripts:compile && node scriptsBuild/genDocument.js && yarn run clean && gatsby build && yarn run playroom:build && yarn run copy",
		"lint": "yarn run lint:js && yarn run lint:css",
		"lint:js": "eslint src --ext .ts,.tsx,.js,.jsx",
		"lint:css": "stylelint 'src/**/*.{ts,tsx,css}'",
		"scripts:watch": "tsc -w --p tsconfig.scripts.json",
		"scripts:compile": "tsc -d --p tsconfig.scripts.json",
		"dev:watch": "yarn run scripts:compile && concurrently \"yarn run scripts:watch\"",
		"playroom:start": "PLAYROOM=true playroom start",
		"playroom:build": "PLAYROOM=true playroom build"
	},
	"devDependencies": {
		"@mdx-js/mdx": "^1.6.22",
