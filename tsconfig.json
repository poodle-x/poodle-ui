{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"allowJs": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "react",
		"baseUrl": "."
	}
}
